{
    "flutter": {
        "platforms": {
            "android": {
                "default": {
                    "projectId": "clubroyale-app",
                    "appId": "1:691812216238:android:b094feb74e94bde0722c3f",
                    "fileOutput": "android/app/google-services.json"
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "clubroyale-app",
                    "configurations": {
                        "android": "1:691812216238:android:b094feb74e94bde0722c3f",
                        "ios": "1:691812216238:ios:5fe504f10d20860b722c3f",
                        "web": "1:691812216238:web:a78824c7d933f12e722c3f"
                    }
                }
            },
            "ios": {
                "default": {
                    "projectId": "clubroyale-app",
                    "appId": "1:691812216238:ios:5fe504f10d20860b722c3f",
                    "uploadDebugSymbols": true,
                    "fileOutput": "ios/Runner/GoogleService-Info.plist"
                }
            }
        }
    },
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [
            {
                "source": "/assets/**",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public, max-age=31536000, immutable"
                    }
                ]
            },
            {
                "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|ico)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public, max-age=31536000, immutable"
                    }
                ]
            },
            {
                "source": "**/*.@(woff|woff2|ttf|otf|eot)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public, max-age=31536000, immutable"
                    }
                ]
            },
            {
                "source": "**/*.@(json|riv|lottie)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public, max-age=86400"
                    }
                ]
            },
            {
                "source": "**/*.@(js|css)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public, max-age=604800"
                    }
                ]
            }
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "firestore": {
        "database": "(default)",
        "location": "nam5",
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "functions": [
        {
            "source": "functions",
            "codebase": "default",
            "ignore": [
                "node_modules",
                ".git",
                "firebase-debug.log",
                "firebase-debug.*.log"
            ]
        }
    ]
}