{
  "name": "taasclub-functions",
  "version": "1.0.0",
  "description": "TaasClub Cloud Functions with Genkit AI",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "deploy": "npm run build && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "genkit:start": "genkit start -- npx tsx --watch src/index.ts"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@genkit-ai/core": "^0.9.0",
    "@genkit-ai/firebase": "^0.9.0",
    "@genkit-ai/googleai": "^0.9.0",
    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.3.2",
    "livekit-server-sdk": "^2.6.0",
    "zod": "^3.23.8"
  },
  "keywords": [
    "firebase",
    "cloud-functions",
    "call-break",
    "genkit",
    "ai"
  ],
  "license": "MIT",
  "devDependencies": {
    "firebase-tools": "^14.27.0",
    "genkit": "^0.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}